<h1>New paste</h1>

<%= error_messages_for :paste %>

<% form_for(@paste) do |f| %>
  <p>
    <b>Language</b><br />
    <%= f.select :language, SYNTAXES %>
  </p>
  <p>
    <b>Code</b><br />
    <%= f.text_area :code %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', pastes_path %>
